{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "tsx server/index.ts",
    "dev:server": "nodemon --watch server --watch shared --ext ts,tsx --signal SIGINT --exec tsx server/index.ts",
    "dev:full": "nodemon --watch server --watch shared --watch client/src --ext ts,tsx,js,jsx --signal SIGINT --exec \"npm run dev\"",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "node dist/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "test": "cross-env NODE_ENV=test jest --config jest.config.json",
    "test:watch": "cross-env NODE_ENV=test jest --config jest.config.json --watch",
    "test:coverage": "cross-env NODE_ENV=test jest --config jest.config.json --coverage",
    "test:api": "cross-env NODE_ENV=test jest --config jest.config.json --testMatch '**/tests/api/**/*.test.ts'",
    "test:integration": "cross-env NODE_ENV=test jest --config jest.config.json --testMatch '**/tests/integration/**/*.test.ts'",
    "test:performance": "k6 run tests/performance/load-test.js",
    "test:e2e": "cross-env NODE_ENV=test playwright test",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "deploy": "cd client && npm run build && cd .. && mkdir -p deploy && cp -r client/dist/* deploy/ && touch deploy/.nojekyll",
    "make:admin": "tsx scripts/make-admin.ts",
    "seed:events": "tsx scripts/seed-events.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
